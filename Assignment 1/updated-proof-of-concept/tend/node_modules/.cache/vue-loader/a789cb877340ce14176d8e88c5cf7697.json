den\" :for=\"label\">{{ label }}</label>\n\n\t\t<select\n\t\t\tv-if=\"type == 'select'\"\n\t\t\t:id=\"label\"\n\t\t\tclass=\"inputBox\"\n\t\t\t:value=\"modelValue\"\n\t\t\t:required=\"required\"\n\t\t\tv-bind=\"validation\"\n\t\t\t@change=\"$emit('update:modelValue', $event.target.value)\"\n\t\t>\n\t\t\t<!-- Create an option that shows the label initially but is unselectable -->\n\t\t\t<option value=\"\" selected disabled>{{ label }}</option>\n\t\t\t<option\n\t\t\t\tv-for=\"option in options\"\n\t\t\t\t:key=\"option.value\"\n\t\t\t\t:value=\"option.value\"\n\t\t\t>\n\t\t\t\t{{ option.text }}\n\t\t\t</option>\n\t\t</select>\n\n\t\t<textarea\n\t\t\tv-else-if=\"type === 'textarea'\"\n\t\t\t:id=\"label\"\n\t\t\tclass=\"inputBox\"\n\t\t\t:value=\"modelValue\"\n\t\t\t:required=\"required\"\n\t\t\t@input=\"$emit('update:modelValue', $event.target.value)\"\n\t\t\t:placeholder=\"label\"\n\t\t\tv-bind=\"validation\"\n\t\t/>\n\n\t\t<input\n\t\t\tv-else\n\t\t\t:id=\"label\"\n\t\t\tclass=\"inputBox\"\n\t\t\t:value=\"modelValue\"\n\t\t\t:required=\"required\"\n\t\t\t@input=\"$emit('update:modelValue', $event.target.value)\"\n\t\t\t:placeholder=\"label\"\n\t\t\t:type=\"type\"\n\t\t\tv-bind=\"validation\"\n\t\t/>\n\t</div>\n</template>\n\n<script>\nexport default {\n\tname: \"Input\",\n\temits: [\"update:modelValue\"],\n\tprops: {\n\t\tmodelValue: String,\n\t\tlabel: String,\n\t\trequired: Boolean,\n\t\toptions: Array,\n\t\tvalidation: Object,\n\t\ttype: {\n\t\t\tdefault: \"text\",\n\t\t\ttype: String,\n\t\t},\n\n\t\tsize: {\n\t\t\tdefault: \"full\",\n\t\t\ttype: String,\n\t\t\tvalidator: (value) => {\n\t\t\t\treturn [\"half\", \"full\"].includes(value);\n\t\t\t},\n\t\t},\n\t},\n\tsetup() {},\n};\n</script>\n\n<style scoped>\n.inputContainer {\n\tdisplay: flex;\n\tjustify-content: center;\n\twidth: 100%;\n}\n\n.inputBox {\n\tbox-sizing: border-box;\n\twidth: 100%;\n\tborder-radius: 10px;\n\tborder: 4px solid #3d6b56;\n\tmargin: 0.5rem;\n\tfont-family: fell, serif;\n\tfont-size: 1rem;\n\tpadding: 0.5rem;\n\tbackground-color: rgba(255, 255, 255, 0.4);\n}\n\n.inputBox:focus {\n\toutline: none;\n\tborder: 4px solid #bf763c;\n}\n\nform.submitted .inputBox:invalid {\n\tborder: 4px solid darkred;\n}\n\n@media (min-width: 800px) {\n\t.halfInput {\n\t\twidth: 50%;\n\t}\n}\n</style>\n"]}]}