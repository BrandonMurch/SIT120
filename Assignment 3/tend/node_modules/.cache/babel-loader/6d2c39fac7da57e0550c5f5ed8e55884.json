m \"vue\";\nimport { getFormattedDate } from \"../composables/getFormattedDate\";\nexport default {\n\tname: \"CardListWithTextbox\",\n\tprops: {\n\t\tvalues: Array,\n\t\tbuttonText: String,\n\t\tscroll: {\n\t\t\ttype: Boolean,\n\t\t\tdefault: false,\n\t\t},\n\t\tdeletable: {\n\t\t\ttype: Boolean,\n\t\t\tdefault: false,\n\t\t},\n\t},\n\tcomponents: { Input, Button, CardText },\n\temits: [\"add\", \"delete\"],\n\tsetup(props, { emit }) {\n\t\tconst newText = ref(\"\");\n\t\tconst add = () => {\n\t\t\tif (newText.value.length > 0) {\n\t\t\t\temit(\"add\", newText.value);\n\t\t\t\t// Reset input after submission.\n\t\t\t\tnewText.value = \"\";\n\t\t\t}\n\t\t};\n\t\treturn { newText, add, getFormattedDate };\n\t},\n};\n</script>\n\n<style scoped>\n@import \"../assets/css/transitionFade.css\";\n\n.card-container {\n\tpadding: 3rem;\n\tmax-height: 80vh;\n\tmax-width: 60vw;\n\tdisplay: flex;\n\tflex-wrap: wrap;\n\tjustify-content: center;\n\tmargin-bottom: 1rem;\n}\n.save-button {\n\tmargin-bottom: 1rem;\n}\n</style>\n"],"sourceRoot":""}]}