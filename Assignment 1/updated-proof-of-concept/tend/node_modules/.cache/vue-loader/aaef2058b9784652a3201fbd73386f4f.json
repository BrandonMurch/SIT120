lass=\"{imageContainer: isLoaded}\">\n        <img class=\"image\" :src=\"imageSource\" :alt=\"title\" @load=\"isLoaded = true\">\n        <div v-if=\"isLoaded\" class=\"image-overlay\">\n            <p class=\"image-hover-text\">{{ title }}</p>\n        </div>\n    </div>\n</template>\n\n<script>\n\nexport default {\n    name: \"ImageCard\",\n    props: {\n        imageSource: String,\n        title: String,\n    },\n    data() {\n        return {\n            isLoaded: false,\n        };\n    }\n}\n</script>\n\n<style scoped>\n    .imageContainer {\n        position: relative;\n        width:250px;\n        border: 1px solid black;\n        border-radius: 20px;\n        box-shadow: 0 5px 5px grey;\n        margin-bottom: 1rem;\n        overflow: hidden;\n    }\n\n    .image {\n        width: 100%;\n        /* \n        There was a small gap that was appearing at the bottom of my images. It seems that the browser treats\n        an image as text. So I used vertical-align to stop this, as discussed here:\n\n        https://stackoverflow.com/questions/10844205/html-5-strange-img-always-adds-3px-margin-at-bottom \n        */\n        vertical-align: middle;\n    }\n\n    .image-overlay {\n        /* Fill parent completely */\n        position: absolute;\n        top: 0;\n        left: 0;\n        width: 100%;\n        height: 100%;\n    }\n\n    .image-overlay:hover {\n        background-color: rgba(0,0,0,0.3);\n        cursor: pointer;\n    }\n    \n    .image-hover-text {\n        display:none;\n    }\n\n    /* Display when card is hovered. */\n    .image-overlay:hover .image-hover-text {\n        display: inline;\n        position:absolute;\n        bottom:10%;\n        left: 5%;\n        text-align: center;\n        width: 90%;\n        color: white;\n        font-size: 2rem;\n        margin: 0;\n    }\n</style>"]}]}