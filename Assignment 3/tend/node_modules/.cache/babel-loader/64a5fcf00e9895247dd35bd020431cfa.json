APA,IAAA,aAAW,EAAA,MAAA,CAAA,CAAA,CAAA,KAAA,MAAA,CAAA,CAAA,CAAA,GAAA,UAAA,MAAA;AAAA,aAAE,MAAA,CAAA,WAAA,GAAW,IAAb;AAAA,KAAA,CAOX;AANA,IAAA,WAAW,EAAE,MAAA,CAAA,WAMb;AALA,IAAA,OAAO,EAAE,MAAA,CAAA,OAKT;AAJA,IAAA,KAAK,EAAE,MAAA,CAAA,KAIP;AAHA,IAAA,eAAa,EAAA,MAAA,CAAA,CAAA,CAAA,KAAA,MAAA,CAAA,CAAA,CAAA,GAAA,UAAG,IAAH;AAAA,aAAY,IAAA,CAAA,KAAA,CAAK,eAAL,EAAuB,IAAvB,CAAZ;AAAA,KAAA,CAGb;AAFA,IAAA,YAAU,EAAE,MAAA,CAAA,UAEZ;AADA,IAAA,UAAQ,EAAE,MAAA,CAAA;AACV,GARF,E,IAAA,E,CAAA,E,iEAAA,CAmBK,EATE,MAAA,CAAA,W,kBADP,YAAA,CAIE,mBAJF,EAIE;UAAA;AAFD,IAAA,KAAK,EAAC,0BAEL;AADA,IAAA,OAAK,EAAA,MAAA,CAAA,CAAA,CAAA,KAAA,MAAA,CAAA,CAAA,CAAA,GAAA,UAAA,MAAA;AAAA,aAAE,IAAA,CAAA,KAAA,CAAK,WAAL,CAAF;AAAA,KAAA;AACL,GAJF,C,iCAUK,EAJE,MAAA,CAAA,W,kBADP,YAAA,CAIE,oBAJF,EAIE;UAAA;AAFD,IAAA,KAAK,EAAC,2BAEL;AADA,IAAA,OAAK,EAAA,MAAA,CAAA,CAAA,CAAA,KAAA,MAAA,CAAA,CAAA,CAAA,GAAA,UAAA,MAAA;AAAA,aAAE,IAAA,CAAA,KAAA,CAAK,YAAL,CAAF;AAAA,KAAA;AACL,GAJF,C,iCAKK,CApBN,C","sourcesContent":["<!--\nDescription: A wrapper for CardImage to enable swiping for mobile devices.\n\nProps:  Refer to CardImage for Prop info.\n\nEmits:\n    swipeLeft       Called when a user swipes left on the image.\n\n    swipeRight      Called when a user swipes right on the image.\n-->\n\n<template>\n\t<div class=\"image-container\">\n\t\t<CardImage\n\t\t\t@imageLoaded=\"imageLoaded = true\"\n\t\t\t:imageSource=\"imageSource\"\n\t\t\t:actions=\"actions\"\n\t\t\t:title=\"title\"\n\t\t\t@performAction=\"(type) => $emit('performAction', type)\"\n\t\t\t@touchstart=\"startSwipe\"\n\t\t\t@touchend=\"endSwipe\"\n\t\t/>\n\t\t<IconLeft\n\t\t\tv-if=\"imageLoaded\"\n\t\t\tclass=\"left-icon direction-icon\"\n\t\t\t@click=\"$emit('swipeLeft')\"\n\t\t/>\n\t\t<IconRight\n\t\t\tv-if=\"imageLoaded\"\n\t\t\tclass=\"right-icon direction-icon\"\n\t\t\t@click=\"$emit('swipeRight')\"\n\t\t/>\n\t</div>\n</template>\n\n<script>\nimport CardImage from \"./CardImage.vue\";\nimport IconLeft from \"./Icons/IconLeft.vue\";\nimport IconRight from \"./Icons/IconRight.vue\";\nimport { ref } from \"vue\";\n\nexport default {\n\tname: \"CardImageSwipeWrapper\",\n\tcomponents: { CardImage, IconLeft, IconRight },\n\tprops: {\n\t\timageSource: String,\n\t\ttitle: String,\n\t\tactions: Array,\n\t},\n\temits: [\"swipeLeft\", \"swipeRight\", \"performAction\"],\n\tsetup(IGNORED, { emit }) {\n\t\tlet swipeStart = 0;\n\t\tconst imageLoaded = ref(false);\n\n\t\tconst startSwipe = (event) => {\n\t\t\tswipeStart = event.changedTouches[0].clientX;\n\t\t};\n\n\t\t// Calculate the difference between the initial touch position, and where the finger lifts up. If this value is negative, it means the user swiped right, otherwise the user swiped left. A buffer of 70 is allowed for small horizontal movements without swiping.\n\t\tconst endSwipe = (event) => {\n\t\t\tlet swipeDifference = swipeStart - event.changedTouches[0].clientX;\n\t\t\tif (swipeDifference < -70) {\n\t\t\t\temit(\"swipeRight\");\n\t\t\t} else if (swipeDifference > 70) {\n\t\t\t\temit(\"swipeLeft\");\n\t\t\t}\n\t\t};\n\n\t\treturn { startSwipe, endSwipe, imageLoaded };\n\t},\n};\n</script>\n\n<style scoped>\n.image-container {\n\tposition: relative;\n}\n\n.direction-icon {\n\tposition: absolute;\n\ttop: 50%;\n\theight: 5rem;\n\twidth: 5rem;\n\ttransform: translate(-20%, -50%);\n\tcursor: pointer;\n\tcolor: #ab6e24;\n}\n\n.left-icon {\n\tleft: 0;\n\ttransform: translate(-20%, -50%);\n}\n\n.right-icon {\n\tright: 0;\n\ttransform: translate(10%, -50%);\n}\n\n@media (min-width: 851px) {\n\t.direction-icon {\n\t\tdisplay: none;\n\t}\n}\n</style>\n"],"sourceRoot":""}]}