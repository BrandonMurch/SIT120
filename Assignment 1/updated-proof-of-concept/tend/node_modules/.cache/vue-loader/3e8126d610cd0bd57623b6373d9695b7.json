) {\n\t\t\t\tfor (let j = 0; j < 7; j++) {\n\t\t\t\t\tlet copy = { ...plantData[j] };\n\t\t\t\t\tcopy.id = copy.id + 7 * i + this.currentImageRound * 7 * 5;\n\t\t\t\t\timages.push(copy);\n\t\t\t\t}\n\t\t\t}\n\n\t\t\treturn images;\n\t\t},\n\n\t\tgetImageData() {\n\t\t\t// New array to trigger watch updates.\n\t\t\tthis.images = [...this.images, ...this.getDataStub()];\n\t\t},\n\t},\n\tcreated() {\n\t\tthis.getImageData();\n\t},\n};\n</script>\n\n<style scoped>\n@import \"../assets/css/transitionFade.css\";\n\n.image-gallery {\n\tdisplay: flex;\n\talign-content: center;\n\tflex-wrap: wrap;\n}\n</style>\n"]}]}